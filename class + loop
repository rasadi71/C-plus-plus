#include<iostream>
using namespace std;
class student{

   public :
       int id ;
       string name;
       float cgpa;

};


int main() {
    student st[100] ;
    int arr[100];
    int n;
    cin >> n;

    for(int i=0;i<n;i++){
        cin >>st[i].id >> st[i].name >> st[i].cgpa ;
        cout << endl;
    }

    cout << "given details below :" <<endl;
    for(int i=0;i<n;i++){
        cout <<st[i].id<<endl<< st[i].name<< endl<<st[i].cgpa<< endl;
        cout<<endl;
    }
}
